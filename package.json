{
  "name": "geek-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\"",
    "lint:es": "eslint ./src --ext .ts,.vue --fix",
    "lint:style": "stylelint src/**/*.{css,less,scss,jsx} --fix --custom-syntax postcss-less",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@geektech/commitlint-config": "^0.0.2",
    "@geektech/eslint-plugin": "^1.0.3",
    "@geektech/stylelint-config": "^0.0.6",
    "@vitejs/plugin-vue": "^3.0.0",
    "eslint": "^8.36.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.6",
    "stylelint": "^15.3.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitepress": "1.0.0-alpha.61",
    "vue-tsc": "^0.38.4"
  }
}
